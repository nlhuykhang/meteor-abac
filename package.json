{
  "name": "meteor-abac",
  "version": "1.0.0",
  "description": "Action-based access control for Meteor",
  "main": "./dist/index.js",

  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
