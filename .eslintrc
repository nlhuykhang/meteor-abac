{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "meteor": true,
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "new-cap": 0,
    "arrow-body-style": 0,
    "no-console": [
      "error",
      {
        "allow": ["warn", "error"]
      }
    ]
  }
}
